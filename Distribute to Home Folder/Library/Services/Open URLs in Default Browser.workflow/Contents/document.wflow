					<string>#!/usr/bin/env perl

# Description: http://daringfireball.net/2010/08/open_urls_in_safari_tabs
# License: See below.
# http://gist.github.com/507356

use strict;
use warnings;
use URI::Escape;

my $text = do { local $/; &lt;&gt; };
my @urls;
my $url_regex = qr{(?xi)
\b
(							# Capture 1: entire matched URL
  (?:
    [a-z][\w-]+:				# URL protocol and colon
    (?:
      /{1,3}						# 1-3 slashes
      |								#   or
      [a-z0-9%]						# Single letter or digit or '%'
      								# (Trying not to match e.g. "URI::Escape")
    )
    |							#   or
    www\d{0,3}[.]				# "www.", "www1.", "www2." … "www999."
    |							#   or
    [a-z0-9.\-]+[.][a-z]{2,4}/	# looks like domain name followed by a slash
  )
  (?:							# One or more:
    [^\s()&lt;&gt;]+						# Run of non-space, non-()&lt;&gt;
    |								#   or
    \(([^\s()&lt;&gt;]+|(\([^\s()&lt;&gt;]+\)))*\)	# balanced parens, up to 2 levels
  )+
  (?:							# End with:
    \(([^\s()&lt;&gt;]+|(\([^\s()&lt;&gt;]+\)))*\)	# balanced parens, up to 2 levels
    |									#   or
    [^\s`!()\[\]{};:'".,&lt;&gt;?«»“”‘’]		# not a space or one of these punct chars
  )
)
};

# Build an AppleScript-syntax list of the URLs in the input text.
while ($text =~ m{$url_regex}g) {
	my $u = $1;
	$u =~ s{([\x{80}-\x{ffff}])}{uri_escape($1)}eg; # Encode non-ASCII characters
	push @urls, qq{"$u"};
}

# Simplification by Dr. Drang (drdrang@gmail.com) on 8/4/2010. See description at
# http://www.leancrew.com/all-this/2010/08/open-urls-in-default-browser/
for (@urls) {
  `/usr/bin/open -g $_; sleep .5`;  # opens without bringing to browser to foreground
  # `/usr/bin/open $_`;  # opens and brings browser to foreground
}

